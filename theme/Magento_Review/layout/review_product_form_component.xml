<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
	<referenceBlock name="product.review.form" remove="true"/>
	<referenceContainer name="content" htmlTag="div">
		<container name="custom.block" htmlClass="review-conclusion" htmlTag="div" after="product.review.form">
			<block name="conclusion" template="Magento_Review::conclusion.phtml"></block>
			<block name="end" template="Magento_Review::end.phtml" before="-"></block>
		</container>
	</referenceContainer>
    </body>
</page>
